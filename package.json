{
  "name": "farmers-market-native",
  "description": "Farmers' market native app",
  "repository": "github:ben-walker/farmers-market-native",
  "license": "BSD-3-Clause",
  "engines": {
    "node": "16"
  },
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "lint": "eslint . --ext .tsx,.ts,.js",
    "typecheck": "tsc --noEmit",
    "generate:graphql": "graphql-codegen --config codegen.yaml",
    "watch:graphql": "CODEGEN_WATCH=true npm run generate:graphql"
  },
  "dependencies": {
    "@apollo/client": "^3.4.7",
    "expo": "~42.0.1",
    "expo-status-bar": "~1.0.4",
    "graphql": "^15.5.1",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-42.0.0.tar.gz",
    "react-native-web": "~0.14.13"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@graphql-codegen/cli": "2.0.1",
    "@types/react": "~17.0.18",
    "@types/react-native": "~0.64.13",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-react-native": "^3.11.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "prettier": "^2.3.2",
    "typescript": "~4.3.5",
    "@graphql-codegen/typescript-react-apollo": "3.0.0",
    "@graphql-codegen/typescript-operations": "2.0.1",
    "@graphql-codegen/typescript": "2.0.0",
    "@graphql-codegen/fragment-matcher": "3.0.0",
    "@graphql-codegen/introspection": "2.0.0"
  }
}
