{
  "name": "farmers-market-native",
  "description": "Farmers market native app",
  "repository": "github:ben-walker/farmers-market-native",
  "license": "BSD-3-Clause",
  "engines": {
    "node": "16"
  },
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "android": "expo start --android",
    "codegen": "graphql-codegen --config codegen.yaml",
    "codegen:watch": "CODEGEN_WATCH=true npm run codegen",
    "dev": "concurrently --kill-others \"npm:codegen:watch\" \"npm:start\"",
    "eject": "expo eject",
    "ios": "expo start --ios",
    "lint": "eslint . --ext .tsx,.ts,.js",
    "start": "expo start",
    "typecheck": "tsc --noEmit",
    "web": "expo start --web"
  },
  "dependencies": {
    "@apollo/client": "^3.4.10",
    "@react-navigation/bottom-tabs": "^6.0.5",
    "@react-navigation/native": "^6.0.2",
    "@react-navigation/native-stack": "^6.1.0",
    "date-fns": "^2.28.0",
    "expo": "~42.0.1",
    "expo-location": "~12.1.2",
    "expo-status-bar": "~1.0.4",
    "graphql": "^15.5.3",
    "react": "^16.13.1",
    "react-dom": "16.14.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-42.0.0.tar.gz",
    "react-native-safe-area-context": "3.2.0",
    "react-native-screens": "~3.4.0",
    "react-native-web": "~0.14.13",
    "styled-components": "^5.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@expo/config": "^6.0.0",
    "@graphql-codegen/add": "^3.1.0",
    "@graphql-codegen/cli": "^2.0.1",
    "@graphql-codegen/fragment-matcher": "^3.0.0",
    "@graphql-codegen/introspection": "^2.0.0",
    "@graphql-codegen/typescript": "^2.1.1",
    "@graphql-codegen/typescript-apollo-client-helpers": "^2.1.4",
    "@graphql-codegen/typescript-operations": "^2.1.1",
    "@graphql-codegen/typescript-react-apollo": "^3.1.1",
    "@types/react": "~17.0.18",
    "@types/react-native": "~0.65.0",
    "@types/styled-components": "^5.1.12",
    "@types/styled-components-react-native": "^5.1.1",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "babel-plugin-styled-components": "^1.13.2",
    "concurrently": "^6.2.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-react-native": "^3.11.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "prettier": "^2.3.2",
    "typescript": "^4.3.5"
  }
}
